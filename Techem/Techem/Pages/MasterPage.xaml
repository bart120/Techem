<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:pages="clr-namespace:Techem.Pages"
             x:Class="Techem.Pages.MasterPage">
    <TabbedPage.Children>
        <NavigationPage Title="Météo">
            <x:Arguments>
                <pages:WeatherPage />
            </x:Arguments>
        </NavigationPage>
        <NavigationPage Title="A propos">
            <x:Arguments>
                <pages:AboutPage />
            </x:Arguments>
        </NavigationPage>
    </TabbedPage.Children>
</TabbedPage>